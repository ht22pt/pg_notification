{
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [
        {
            "taskName": "build-babel",
            "args": [ "run", "build-babel" ],
            "isBuildCommand": true
        },
        {
          "taskName": "build",
          "args": [ "run", "build" ],
          "isBuildCommand": true
      },
        {
          "taskName": "cp -Rf ./src/emails ./dist",
          "args": [ "run", "copy-email-template" ],
          "isBuildCommand": true
      }
    ]
}
